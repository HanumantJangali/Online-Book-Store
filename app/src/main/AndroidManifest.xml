<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinebookstore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineBookStore">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Second_Book_List" />
        <activity android:name=".CreditCard" />
        <activity android:name=".GooglePay" />
        <activity android:name=".Payment" />
        <activity android:name=".Transactions" />
        <activity android:name=".Register">
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".BookList"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BookOrder"
            android:parentActivityName=".BookList" />
    </application>

</manifest>